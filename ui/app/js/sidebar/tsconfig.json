        "./Sidebar.ts",
        "./filter/DateTimeInput.ts",
        "./filter/FilterSidebar.ts",
        "./filter/LogEventFilter.ts",
        "./filter/MsgProcessedFilter.ts",
        "./filter/ProcessingState.ts"
    ]
